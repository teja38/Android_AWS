<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.familyroom.FamilyRoomFragment">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background2"
        tools:context=".ui.parent.FragmentParentRoom">

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="4dp"
            app:columnCount="4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:rowCount="5">

            <Button
                android:id="@+id/room3_wall_light"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/roof_light"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <Button
                android:id="@+id/room3_dim_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/light2_on"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <Button
                android:id="@+id/room3_fan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/fan_off"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <Button
                android:id="@+id/room3_ac"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_off"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <Button
                android:id="@+id/room3_socket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/socket_3"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />
            <Button
                android:id="@+id/bathroom3_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/bathroom3_light_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bathroomlightoff"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />
            <Button
                android:id="@+id/bathroom3_geyser"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/bathroom3_geyser_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/heateroff"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <Button
                android:id="@+id/room3_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/button_2"
                android:tag="OFF"
                android:text="OFF"
                android:textSize="18sp"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

            <ImageView
                android:id="@+id/room3_tv_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/tv_icon_1"
                app:layout_columnWeight="0.25"
                app:layout_gravity="fill"
                app:layout_rowWeight="0.25" />

        </androidx.gridlayout.widget.GridLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>